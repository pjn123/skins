	<script file="scripts/centerobject.maki" param="player.main.config.content,1,1"/>
	
	<Wasabi:fileSelect x="0" y="0" w="200" h="1"/>
	
	<script file="scripts\config.maki"/>
</groupdef>

<groupdef id="player.pagescroller">
	<button id="player.button.page.left"
		x="20" y="0"
		image="player.button.page.left"
		downimage="player.button.page.left.down"
		rectrgn="1"/>
		
	<button id="player.button.page.right"
		x="-72" y="0" relatx="1"
		image="player.button.page.right"
		downimage="player.button.page.right.down"
		rectrgn="1"/>
</groupdef>

<groupdef id="player.main.pbuttons">
	<button 
		id="Minimize"
		x="-137" y="20" relatx="1"
		action="minimize" 
		image="player.main.close"
		downimage="player.main.closemin"
		hoverimage="player.main.closemin"
		rectrgn="1"
		tooltip="Minimize"
		/>
	<button 
		id="Close"
		x="-95" y="20" relatx="1"
		action="CLOSE" 
		image="player.main.close"
		downimage="player.main.closed"
		hoverimage="player.main.closed"
		rectrgn="1"
		tooltip="Close"
		/>
</groupdef>

<layout id="normal" desktopalpha="0"
	minimum_h="480" minimum_w="640" maximum_h="3864" maximum_w="3152" move="1" >

	<layer id="bg.noalpha" x="0" y="0" w="0" h="0" relatw="1" relath="1" image="player.main.background.noalpha" sysregion="1" move="1"/>
	
	<CustomBG x="0" y="0" w="0" h="0" relatw="1" relath="1"/>

	<group id="player.main.cms" x="0" y="0" w="100" h="0" relatx="2" relatw="2" relath="1"/>
	<group id="player.main.pl" 	x="0" y="0" w="100" h="0" relatx="2" relatw="2" relath="1" visible="0"/>
	<group id="player.main.config" 	x="0" y="0" w="100" h="0" relatx="2" relatw="2" relath="1" visible="0"/>
	
	<group id="player.cbuttons" x="0" y="-100" relaty="1" w="227" h="165"/>
	<script file="scripts/centerobject.maki" param="player.cbuttons,1,0"/>
	
	<group id="player.main.pbuttons" x="0" y="0" w="100" h="0" relatx="2" relatw="2" relath="1"/>
	
	<group id="player.pagescroller" x="0" y="0" w="0" h="59" relatw="1"/>
	<script file="scripts/centerobject.maki" param="player.pagescroller,0,1"/>
	
	<script file="scripts/pagescroll.maki" param="player.main.cms,player.main.pl,player.main.config"/>
	
	<layer image="player.main.background.noalpha" x="-20" y="-20" relatx="1" relaty="1" w="20" h="20" resize="bottomright"/>

</layout>
