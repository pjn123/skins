








<groupdef id="main.display.content" name="Main">

 


<AnimatedLayer 
     x="30" y="10" 
     w="387" h="493"
    
     id="animationlayer"       
     image="anim"                 
     move="1"                  
	 speed="80"              
     autoplay="1"             
     autoreplay="1"           
	 sysregion="1"
       />
 <layer id="pumpkin" image="player.main.pumpkin"  sysregion="1" x="56" y="400" move="1"/>
<AnimatedLayer
          id="Vis.Animation"
          x="56" y="400"
          w="332" h="287"
          image="player.main.vis"
          alpha="0"  move="1"
		  
         
         />  


<script id="customvis" file="scripts/customvis.maki"/>



<button 
 id="shade"
 x="25" y="460" sysregion="1"
 action="SWITCH" param="normalc" 
 image="ml"
 downimage="ml"
 hoverimage="ml"
 rectrgn="1"
 tooltip="Turn to Back"
/>
</groupdef>
<groupdef id="main.control.buttons" name="Cbuttons">
<button 
  id="Minimize"
 x="58" y="26"
 action="MINIMIZE" 
 image="player.main.min"
 downimage="player.main.mind"
 hoverimage="player.main.mind"
 rectrgn="1"
 tooltip="Minimize"
/>

<button 
 id="Close"
 x="41" y="25"
 action="CLOSE" 
  image="player.main.close"
 downimage="player.main.closed"
 hoverimage="player.main.closed"
rectrgn="1"
 tooltip="Terminate"
/>
<button 
 id="normal2"
 action="SWITCH" param="normal2" rectrgn="1"
  x="297" y="5" image="fglobe" 
 downimage="fglobe" 

  
 tooltip="ON FIRE"
 />

<button 
 id="Stop"
 x="272" y="195"
 action="STOP"  alpha="180"
 image="player.main.stop"  
 downimage="player.main.stopd"
    rectrgn="1"
 activeImage="player.main.stopd"
 hoverImage="player.main.stopd"
 tooltip="Stop"
 
/>
<layer id="player.main.stopd" image="player.main.stopd" x="272" y="195" ghost="1"/>

	
<script file="scripts/hoverglow.maki" param="Stop,player.main.stopd"/>
<button 
 id="Next"
 x="242" y="196"
action="NEXT"  alpha="200"
image="player.main.forward"
 downimage="player.main.forwardd"
    rectrgn="1"
 activeImage="player.main.forwardd"
 hoverImage="player.main.forwardd"
 tooltip="Next Track"
 
/>
<layer id="player.main.forwardd" image="player.main.forwardd"  x="242" y="196" ghost="1"/>

	
<script file="scripts/hoverglow.maki" param="Next,player.main.forwardd"/>
<button 
 id="Rev"
  x="181" y="197"
 action="PREV"  alpha="200"
image="player.main.rev" 
 downimage="player.main.revd"
    rectrgn="1"
 activeImage="player.main.revd"
 hoverImage="player.main.revd"
 tooltip="Prev Track"
 
/>
<layer id="player.main.revd" image="player.main.revd"   x="181" y="197" ghost="1"/>

	
<script file="scripts/hoverglow.maki" param="Rev,player.main.revd"/>
<button 
 id="Eject"
 x="158" y="194"
 action="EJECT"  alpha="200"
 image="player.main.eject"
 downimage="player.main.ejectd"
    rectrgn="1"
 activeImage="player.main.ejectd"
 hoverImage="player.main.ejectd"
 tooltip="Get Song"
 
/>
<layer id="player.main.ejectd" image="player.main.ejectd" x="158" y="194" ghost="1"/>

	
<script file="scripts/hoverglow.maki" param="Eject,player.main.ejectd"/>
<button 
 id="Play"
 x="212" y="197"
 action="PLAY" alpha="200"
 image="player.main.play"
downimage="player.main.playd"
    rectrgn="1"
 activeImage="player.main.playd"
 hoverImage="player.main.playd"
 tooltip="Pause"
 
/>
<layer id="player.main.playd" image="player.main.playd" x="212" y="197" ghost="1"/>

	<text
	id="songticker.text" ticker="1"
     display="songname" default=""
	x="110" y="10" 
	w="224" h="35"  antialias="1"
	font="player.font.ticker" fontsize="25" align="center" 
	color="221,136,26"
   />
	<text
	id="Timer" 
	display="time"
	x="203" y="40" 
	w="60" h="30" bold="1"
	font="player.font.ticker" fontsize="28" align="left"  
    color="221,136,26"
	 
  
	timecolonwidth="5"
  />	
  
<script file="scripts/hoverglow.maki" param="Play,player.main.playd"/>


</groupdef>
<layout id="normal" background="player.main.background" 
alphabackground="player.main.background" >

  
 <group id="main.display.content" w="600" h="800"/>
<group id="main.control.buttons" y="400" w="500" h="400" visible="0"/>
 <script id="display" file="scripts/display.maki" />

</layout>
