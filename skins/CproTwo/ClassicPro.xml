<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ClassicPro version="2.00" engine="two">
	
	<!--
	TextSettings:
	For a list of parameters you can change for this <text/> object:
		http://wiki.winamp.com/wiki/XML_GUI_Objects#.3Ctext.2F.3E_.26_.3CWasabi:Text.2F.3E
	-->
	<TextSettings>
		<Style id="normal.statusbar.text"
			fontsize="14" y="1" antialias="0" bold="0"
		/>
		<Style id="normal.fileinfo.text"
			antialias="1" bold="0" italic="0"
		/>
		
		<Style id="normal.info.title.text"
			bold="1"
		/>
		<Style id="normal.info.artist.text"
			bold="1"
		/>
		<Style id="normal.info.timesmall.text"
			bold="1" fontsize="15" h="16" x="100"   
		/>

		<!--
		Use the display param to move the text away from the right side.
		i.e. display="10" will move the text left by 10 pixels.
		The reason (if interested :P) for this work around is because the x/w position is automatically set by scripts here. (and must use an internal param so can't use clever name like width_from_side="15" ;)
		Please note that this is only applicable on the 'timebig', 'bitrate' and 'news'!
		-->
		<Style id="normal.info.timebig.text"
			bold="1" display="0"
		/>
		<Style id="normal.info.news.text" 
			bold="1" display="0"
		/>
		<Style id="normal.info.bitrate.text" 
			bold="1" display="0"
		/>
		
	</TextSettings>

	<Visualization>
		<BeatVis>
			<customvis name="Bars"/>
			<customvis name="Llama"/>
			<customvis name="Nyan Cat"/>
		</BeatVis>
	</Visualization>
	
	<About:Skin>
		<Skinner
			name="Mike the Llama"
			alias="Llama"
			dob="6;1;1979"
			country="Somewhere"
			about="Thanks for using my skin!\nHere you can write more about the skin!"
		/>
	</About:Skin>

</ClassicPro>