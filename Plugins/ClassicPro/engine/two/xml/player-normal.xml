	<button id="two.playback.play" x="95" y="0" action="PLAY" image="playback.button.play.1" hoverimage="playback.button.play.2" downimage="playback.button.play.3"/>
	<button id="two.playback.paus" x="95" y="0" action="PAUSE" image="playback.button.paus.1" hoverimage="playback.button.paus.2" downimage="playback.button.paus.3"/>
	<button id="two.playback.next" x="129" y="0" action="NEXT" image="playback.button.next.1" hoverimage="playback.button.next.2" downimage="playback.button.next.3"/>
	<button x="161" y="0" action="STOP" image="playback.button.stop.1" hoverimage="playback.button.stop.2" downimage="playback.button.stop.3"/>
